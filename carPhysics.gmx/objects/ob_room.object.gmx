background_vspeed[1]=global.speed;
//stutter cam
if(global.stutter&gt;0){
background_vspeed[0]=0;
background_vspeed[1]=0;
global.speed=0;

}
global.stutter--;
//escape
if(global.escape==true){
background_vspeed[0]=0;
background_vspeed[1]=0;
global.speed=0;

}

